{
  "name": "DomainStayDurationChecker",
  "description": "Check stay duration of each domains",
  "version": "0.1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background/background.js", "models/tab-info.js"]
  },
  "page_action": {
    "default_icon": {
      "16": "images/pika16.png",
      "19": "images/pika19.png",
      "38": "images/pika38.png",
      "48": "images/pika48.png",
      "128": "images/pika128.png"
    },
    "default_title": "Domain Stay Duration",
    "default_popup": "pages/main/main.html"
  },
  "permissions" : [
    "tabs", "storage", "alarms"
  ],
  "icons": {
    "16": "images/pika16.png",
    "19": "images/pika19.png",
    "38": "images/pika38.png",
    "48": "images/pika48.png",
    "128": "images/pika128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-6V6mZVgewHoMDuK3KefWp7Llrv+UWOWARbqIsksfQL8=' 'sha256-iQxK+icJV50tH5bZbyqQPefnFtMTIwsEW9G+uFMmtDg='; object-src 'self'"
}